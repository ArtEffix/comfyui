{
  "daemon": true,
  "run": [
    {
      "method": "shell.run",
      "params": {
        "venv": "env",
        "path": "app",
        "env": {
          "PYTORCH_MPS_HIGH_WATERMARK_RATIO": "0.0"
        },
        "message": "python main.py --cpu",
        "on": [
          {
            "event": "/http:\/\/[0-9.:]+/",
            "done": true
          }
        ]
      }
    },
    {
      "method": "local.set",
      "params": {
        "url": "{{input.event[0]}}"
      }
    },
    {
      "method": "proxy.start",
      "params": {
        "uri": "{{local.url}}",
        "name": "Local Sharing"
      }
    }
  ]
}
